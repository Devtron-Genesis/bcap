+    bottom: 18px;
+    z-index: 9999;
+    left: 38px;
+}
+
+/*NEWS Syle */
+ul.pgafu-filter li a {
+    background: none;
+    text-transform: uppercase;
+    font-size: 16px;
+    color: #1c1c1c;
+    font-family: 'avenirheavy';
+    display: block;
+    text-decoration: none;
+    padding: 9px 22px;
+}
+.pgafu-filter li.pgafu-active-filtr a {
+    background: none !important;
+    color: #139c49 !important;
+}
+.pgafu-post-grid-main.pgafu-design-2 .pgafu-post-grid-content {
+    border-bottom: 3px solid #139c49 !important;
+    padding-bottom: 30px !important;
+}
+
+.pgafu-content-above-image h2 a {
+    color: #1c1c1c !important;
+}
+.pgafu-post-short-content {
+    color: #1c1c1c !important;
+}
+
+.pgafu-content-above-image h2 a:hover {
+    color: #139c49 !important;
+}
+
+.page-id-33 .news_wrap {
+    padding: 10px 70px;
+}
+span.pgafu-time {
+    font-style: normal !important;
+    color: #1c1c1c !important;
+    font-size: 15px !important;
+}
+span.pgafu-user-img {
+    font-style: normal !important;
+    font-family: 'avenirheavy' !important;
+    font-weight: 400 !important;
+    color: #139c49 !important;
+    font-size: 15px !important;
+}
+
+/*PAGES STYLE MENU NEWS*/
+.team-template-default ul#menu-main-top-menu li a {
+    color: #000;
+}
+
+ul.pgafu-filter {
+    margin-bottom: 25px;
+}
+
+
+/*Portfolio Style */
+.container.portfolio {padding-left: 130px;padding-right: 130px;}
+
+.container.connect {
+    padding-left: 125px;
+    padding-right: 125px;
+    padding-top: 30px;
+    padding-bottom: 30px;
+}
